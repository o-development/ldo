{
  "name": "@ldo/rdf-utils",
  "version": "1.0.0-alpha.30",
  "description": "Some RDF Utilities to support LDO librariers",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json && node ../../scripts/rename-to-cjs.js",
    "build:esm": "tsc --project tsconfig.esm.json",
    "watch": "tsc --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": " npm run build",
    "lint": "eslint src/** --fix --no-error-on-unmatched-pattern",
    "remove-dist": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/o-development/ldo.git"
  },
  "author": "Jackson Morgan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/o-development/ldo/issues"
  },
  "homepage": "https://github.com/o-development/ldo/tree/main/packages/rdf-utils#readme",
  "devDependencies": {
    "@rdfjs/types": "^1.0.1",
    "@types/jsonld": "^1.5.9"
  },
  "dependencies": {
    "@rdfjs/data-model": "^1.2.0",
    "n3": "^1.17.1",
    "rdf-string": "^1.6.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "840910c56ec3f61416f031cc76771a5673af6757"
}
